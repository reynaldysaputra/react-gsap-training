       <div style={{width:'100%',height:'200px',background:\"red\"}}></div>\n            </Tween>\n         </Reveal>\n\n         <Reveal repeat trigger={<div/>}>\n            <FadeInLeft>\n               <h3>Hello i'am back :)</h3>\n            </FadeInLeft>\n         </Reveal>\n      </div>\n   )\n}\n\nexport default AppsReveal;","/Volumes/DATA/BasicDeveloper/Advance/Framework/ReactJS/scrollmagic-gsap/src/1.BasicGSAP/02_tween/apps.jsx",[],"/Volumes/DATA/BasicDeveloper/Advance/Framework/ReactJS/scrollmagic-gsap/src/1.BasicGSAP/03_timeline/apps.jsx",["104","105"],"import React, { Fragment, useRef } from 'react';\nimport {Controls, PlayState, Timeline,Tween} from 'react-gsap';\nimport styled from 'styled-components';\nimport {Box1} from './boxComponent';\nimport {ThreeText} from './boxComponent';\nimport style from './style.module.css';\n\nconst Box = styled.div`\n   width:100px;\n   height:100px;\n   background-color:red;\n`;\n\nfunction Timelines(){\n   const ref = useRef();\n\n   return(\n      <Fragment>\n         <Controls playState={PlayState.pause}>\n            <Timeline\n               target={<Box1/>}\n            >\n               <Tween from={{opacity:0}} to={{opacity:1}} playState={PlayState.play} />\n               <Tween to={{x:500, backgroundColor:'green'}} />\n               <Tween to={{rotate:180}} />\n            </Timeline>\n         </Controls>\n\n         <Controls playState={PlayState.pause}>\n            <Timeline\n               target={<div className={style.box1}></div>}\n            >\n               <Tween from={{x:'-100vw', opacity:0}} to={{x:'20vw', opacity:1}} />\n               <Tween to={{width:'100%'}}>\n                  <div className={style.box2}></div>\n               </Tween>\n            </Timeline>\n         </Controls>\n\n         <Controls playState={PlayState.pause}>\n            <Timeline\n               target={(\n                  <Fragment>\n                     <div className={style.box1}>Component 1</div>\n                     <div className={style.box2} style={{color:'white'}}>Component 2</div>\n                  </Fragment>\n               )}\n               duration={.8}\n            >\n               <Tween to={{x:'5vw'}} target={0} />\n               <Tween to={{x:'10vw'}} target={1} />\n               <Tween to={{x:'15vw'}} target={0} />\n               <Tween to={{x:'20vw'}} target={1} />\n               <Tween to={{x:'20vw'}} target={0} />\n            </Timeline>\n         </Controls>\n\n         <Controls playState={PlayState.pause}>\n            <Timeline target={<ThreeText/>}>\n               <Tween from={{x:'-30vw', opacity:0}} to={{x:'30vw',opacity:1}} target='text1' position='0' />\n               <Tween from={{ x: '-100vw' }} to={{x:'30vw'}} target=\"text2\" position=\"1\" stagger={.1}  />\n            </Timeline>\n         </Controls>\n\n         <Controls playState={PlayState.pause}>\n            <Timeline\n               target={<div className={style.box1}></div>}\n               labels={[\n                  {\n                     label : 'animate1',\n                     position:0\n                  },\n                  {\n                     label : 'animate2',\n                     position:1\n                  },\n                  {\n                     label : 'animate3',\n                     position:2\n                  },\n                  {\n                     label : 'animate4',\n                     position:3\n                  }\n               ]}\n            >\n               <Tween from={{x:'70vw'}} to={{x:'50vw', opacity:0}} position='animate2' />\n               <Tween to={{rotate:180, scale:1.5, opacity:1}} position='animate3' />\n               <Tween to={{x:'70vw', opacity:1}} position='animate1' />\n               <Tween to={{opacity:1, scale:.8, x:'0'}} position='animate4' />\n            </Timeline>\n         </Controls>\n      </Fragment>\n   )\n}\n\nexport default Timelines;","/Volumes/DATA/BasicDeveloper/Advance/Framework/R